    "telemetry-events-quantify": "1.0.0",
    "telemetry-events-trace": "0.2.0"
  },
  "dependencies": {
    "jest": "23.4.1",
    "rxjs": "6.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:tristanls/module-telemetry-policy.git"
  },
  "keywords": [
    "telemetry",
